{
"name": "标签切换",
"version": "1.0.0",
"manifest_version": 2,
"description": "快速切换标签,一键自动下一个标签或者上一个标签。",
"browser_action": {
	"default_title": "标签切换",
	"default_icon": "icon_128.png"
},
"background": {
       "scripts": ["background.js"]
},
"icons": {
   	"16": "icon_16.png", 
    "48": "icon_48.png",
    "128": "icon_128.png"
},
"permissions": [
	"http://*/*",
	"tabs",
	"unlimitedStorage",
	"webNavigation", 
	"webRequest", 
	"webRequestBlocking" 
],
"content_scripts": [
    {
	  "matches": ["http://*/*","https://*/*"], 
	  "run_at": "document_end",
      "js": ["jquery.js","content.js"]
    }
]
}